algoritmo "015_L03_EX_05_v1"

//aprensetar o total da soma dos quadrados dos números inteiros de 2 a 5

var

   n1, n2 : real
   i : real
   soma : real

inicio

      //
      //ENQUANTO_PARA
      //
      n1<-2^2
      n2<-5^2
      i<-1
      soma<-0
      
      enquanto (n1<=n2) faca
           escreva(n1)
           n1<-n1+1
           
      fimEnquanto

fimalgoritmo